database diteca
screen 
{

              
             Código Registro: [c000]   

        Impuesto Facturación: [c001 ]%
             Nombre Impuesto: [c002      ]
     Moneda Base Facturación: [a0] [c003            ] Decimales: [a ]
              Moneda Alterna: [a1] [c004            ] Decimales: [b ]
            
               Ingresado Por: [c005      ]
            Fecha de Ingreso: [c006               ]
}
end
tables
gent000
attributes
a0 = gent000.g00_moneda_base, REVERSE, AUTONEXT, UPSHIFT,
	WIDGET = 'FIELD_BMP', CONFIG = 'lista.bmp F2';
a1 = gent000.g00_moneda_alt, AUTONEXT, UPSHIFT,
	WIDGET = 'FIELD_BMP', CONFIG = 'lista.bmp F2';
c000 = gent000.g00_serial, REVERSE, NOENTRY,
	WIDGET = 'FIELD_BMP', CONFIG = 'lista.bmp F2';
c001 = gent000.g00_porc_impto, REVERSE, FORMAT = '#&.##';
c002 = gent000.g00_label_impto, REVERSE, UPSHIFT;
c003 = formonly.moneda1, NOENTRY;
a    = gent000.g00_decimal_mb, REVERSE, AUTONEXT, FORMAT = '&', 
                               INCLUDE = (0,1,2);
c004 = formonly.moneda2, NOENTRY;
b    = gent000.g00_decimal_ma, FORMAT = '&', AUTONEXT, INCLUDE = (NULL,0,1,2);
c005 = gent000.g00_usuario, REVERSE, NOENTRY, UPSHIFT;
c006 = gent000.g00_fecing, REVERSE, NOENTRY;
end
