DATABASE aceros
SCREEN 
{


       Código de Localidad: [c000] [a                        ]
          Código de Modulo: [c0]   [b                        ]
                    Bodega: [c1]

                      Tipo: [c2]
                    Nombre: [c001                            ]		
                    Número: [c002    ]

             Ingresado Por: [c003       ]
          Fecha de ingreso: [c004               ]


}
end
TABLES
gent015
ATTRIBUTES
c000 = gent015.g15_localidad, REVERSE,
       WIDGET = 'FIELD_BMP', CONFIG = 'lista.bmp F2';
a    = FORMONLY.nom_loc TYPE VARCHAR, NOENTRY;
c0   = gent015.g15_modulo, REVERSE, UPSHIFT,
       WIDGET = 'FIELD_BMP', CONFIG = 'lista.bmp F2';
b    = FORMONLY.nom_mod TYPE VARCHAR, NOENTRY;
c1   = gent015.g15_bodega, REVERSE, UPSHIFT;
c2   = gent015.g15_tipo, REVERSE, UPSHIFT;
c001 = gent015.g15_nombre,REVERSE, UPSHIFT;
c002 = gent015.g15_numero,REVERSE;
c003 = gent015.g15_usuario, REVERSE, UPSHIFT, NOENTRY;
c004= gent015.g15_fecing, REVERSE, NOENTRY;
end
