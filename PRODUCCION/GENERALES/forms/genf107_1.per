DATABASE aceros
SCREEN
{


                Banco: [c001] [c008                                     ]

     Número de Cuenta: [c002           ]            Estado: [a|c009     ] 
        
          Tipo Cuenta: [b             ]    Moneda: [a0] [c010           ]

    
           Pago Roles: [c]     
           Dirigido a: [c003                                            ]
    Auxiliar Contable: [c004        ] [c011                             ]
   Númeración cheques: [c005     ]
        
        Ingresado Por: [c006      ]
     Fecha de Ingreso: [c007               ]

}
end
TABLES
gent009
ATTRIBUTES
c001 = gent009.g09_banco, REVERSE, 
	WIDGET='FIELD_BMP', CONFIG='lista.bmp F2';
c002 = gent009.g09_numero_cta, UPSHIFT, REVERSE,
	WIDGET='FIELD_BMP', CONFIG='lista.bmp F2';
a = gent009.g09_estado, AUTONEXT, UPSHIFT, REVERSE, NOENTRY, 
	COMMENT='A ACTIVO B BLOQUEADO';
b = gent009.g09_tipo_cta, AUTONEXT, REVERSE, DEFAULT='A',
	WIDGET='RADIO', CONFIG='A Ahorro C Corriente';
a0 = gent009.g09_moneda, UPSHIFT, REVERSE, AUTONEXT,
	WIDGET='FIELD_BMP', CONFIG='lista.bmp F2';
c = gent009.g09_pago_roles, AUTONEXT, REVERSE, DEFAULT='N',
	WIDGET='CHECK', CONFIG='S N acknowledge',
        COMMENT = 'Indicar si la cuenta va a ser usada para pagos a empleados';
c003 = gent009.g09_atencion_rol, UPSHIFT,
        COMMENT = 'Indicar a quien va dirigida la carta al banco para pagos a empleados';
c004 = gent009.g09_aux_cont, REVERSE, UPSHIFT,
	WIDGET='FIELD_BMP', CONFIG='lista.bmp F2';
c005 = gent009.g09_num_cheques, REVERSE, INCLUDE=(0 TO 999999),
        COMMENT = 'Control automatico de numeración de cheques';
c006 = gent009.g09_usuario, REVERSE, UPSHIFT, NOENTRY;
c007 = gent009.g09_fecing, REVERSE, NOENTRY;
c008 = FORMONLY.n_banco, UPSHIFT, NOENTRY;
c009 = FORMONLY.n_estado, UPSHIFT, NOENTRY;
c010 = FORMONLY.n_moneda, UPSHIFT, NOENTRY;
c011 = FORMONLY.n_cuenta, UPSHIFT, NOENTRY;
end
