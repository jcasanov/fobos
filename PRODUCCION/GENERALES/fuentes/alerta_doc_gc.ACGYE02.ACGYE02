echo $MSG_FA|mail -s "ALERTA !!!"  nelson  francisco
fi

if [ "$MSG_NC" ]; then
echo $MSG_NC|smbclient  -I 192.168.4.100 -M sistemas
echo $MSG_NC|smbclient  -I 192.168.4.98 -M fobosadmin
