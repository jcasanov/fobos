set `logname`
if [ $1 != fobos ] ; then
	echo 'Solo usuario fobos puede correr este proceso.'
	exit
fi
clear
export DIR="/FOBOS/PRODUCCION/TRANSMISION"
echo 'CARGA DE STOCK DE DITECA-GYE A DITECA-QTO'
echo '--------------------------------------------------------------'
echo ''
echo 'Este proceso descarga de Diteca Guayaquil el stock de las'
echo 'bodegas y lo carga a la base de datos de Diteca Quito.'
echo 'Se recomienda ejecutar este proceso al final del dia o a primera hora.'
echo ''
echo -n 'Conecte modem a Diteca-Quito y presione <Enter>'
read a
echo ''
cd
echo 'Entrando al directorio: ' $DIR 
cd $DIR
export INFORMIXSERVER=ol_server
echo 'Ejecutando programa de descarga de datos de la base diteca...'
fglrun baja_stock_gye prueba_gm
echo 'Copiando file tr_a_quito_items.txt a Quito...'
rcp tr_a_quito_items.txt.gz server_qm:/FOBOS/PRODUCCION/TRANSMISION/

echo 'Copiando file tr_a_quito_stock.txt a Quito...'
rcp tr_a_quito_stock.txt.gz server_qm:/FOBOS/PRODUCCION/TRANSMISION/

echo 'Copiando file tr_a_quito_precios.txt a Quito...'
rcp tr_a_quito_precios.txt.gz server_qm:/FOBOS/PRODUCCION/TRANSMISION/

export INFORMIXSERVER=ol_serverqm
echo 'Ejecutando programa que carga datos a la base diteca_qm...'
rsh server_qm /FOBOS/PRODUCCION/TRANSMISION/act_stock_qm
#echo 'Proceso terminado Ok, Cargando Stock de Quito en Guayaquil'
#rcp server_qm:/FOBOS/PRODUCCION/TRANSMISION/tr_stock_a_gye.txt.gz .
#gunzip -f tr_stock_a_gye.txt.gz

#export INFORMIXSERVER=ol_server

#echo 'Antes del carga_stock.4gl'
#fglrun carga_stock_uio diteca
echo 'Proceso terminado Ok.'
echo '*** OJO  No olvide cerrar conexion del modem, Presione Enter ***'
read a
