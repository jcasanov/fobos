export DIR=$PWD
cd $DIR
for modulo in ACTIVOS CAJA COBRANZAS COMPRAS CONTABILIDAD GENERALES LIBRERIAS MENU NOMINA REPUESTOS SRI TALLER TESORERIA
do
	cd $modulo
	cd fuentes
	echo $PWD
	for j in ???p???.4gl
	do
		echo "Copiando $modulo/fuentes/$j a $HOME/PRODUCCION/$modulo/fuentes/"
		cp $j $HOME/PRODUCCION/$modulo/fuentes/
	done
	cd ../forms
	for j in ???f*.per
	do
		echo "Copiando $modulo/forms/$j a $HOME/PRODUCCION/$modulo/forms/"
		cp $j $HOME/PRODUCCION/$modulo/forms/
	done
	cd ../..
done
echo " "
echo "Copiado todo el FOBOS en $HOME/PRODUCCION/ Ok."
