DATABASE aceros
SCREEN
{


           Registro No: [f001       ]	               Estado: [a|a1       ]

              Division: [f003 ] [a2                      ]
       Indice Rotación: [a0] [a3                         ]
             Tipo Item: [f004 ] [a4                      ]
	    Porcentaje: [f002     ]%	                 

                Moneda: [b1              ]       Rubro Base: [b          ]


    Fecha de Procesado: [f007               ]
         Ingresado Por: [f005      ] 
      Fecha de Ingreso: [f006               ]

}
TABLES
rept032
ATTRIBUTES
f001 = rept032.r32_numreg, REVERSE, NOENTRY,
	WIDGET = 'FIELD_BMP', CONFIG = 'lista.bmp F2';
a    = rept032.r32_estado, UPSHIFT,REVERSE, NOENTRY,
	COMMENTS = 'A Activo  P Procesado';
a1   = FORMONLY.tit_estado TYPE VARCHAR, NOENTRY;
b1   = rept032.r32_moneda, REVERSE, DEFAULT = 'A', 
	WIDGET = 'RADIO', CONFIG = 'A "Moneda Base" B "Moneda Alterna"',
	COMMENT = 'Moneda a la que se le hara el cambio de precio. ';
b    = rept032.r32_rubro_base, REVERSE, 
	WIDGET = 'RADIO', CONFIG = 'P Precio C Costo',
	COMMENT = 'Rubro Base sobre el que se va a calcular el nuevo valor. ';
f002 = rept032.r32_porc_fact, REVERSE, FORMAT = '##,##&.##', 
	INCLUDE = (0.00 TO 99999.99),
	COMMENT = 'Porcentaje para cambio de precios. ';
f003 = rept032.r32_linea, REVERSE, UPSHIFT,
	WIDGET = 'FIELD_BMP', CONFIG = 'lista.bmp F2';
a2   = FORMONLY.nom_lin TYPE VARCHAR, NOENTRY;
a0   = rept032.r32_rotacion, UPSHIFT, AUTONEXT,
	WIDGET = 'FIELD_BMP', CONFIG = 'lista.bmp F2';
a3   = FORMONLY.nom_rot TYPE VARCHAR, NOENTRY;
f004 = rept032.r32_tipo_item,
	WIDGET = 'FIELD_BMP', CONFIG = 'lista.bmp F2';
a4   = FORMONLY.nom_tipo TYPE VARCHAR, NOENTRY;
f005 = rept032.r32_usuario, REVERSE, NOENTRY;
f006 = rept032.r32_fecing, REVERSE, NOENTRY;
f007 = rept032.r32_fecpro, REVERSE, NOENTRY;
end
