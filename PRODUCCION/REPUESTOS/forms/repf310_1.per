DATABASE formonly
SCREEN
{
          Período: [c001      ]  [c002      ]
           Moneda: [c3]     [c004                ]     [c                ]
           Bodega: [c5]     [c006                ]                            
   Línea de Venta: [c007 ]  [c008                ]                            
     Tipo de Item: [c009 ]  [c010                ]
  Indice Rotación: [x1]     [c012                ]

 [y1|c030           |c031                     |c032    |c033 |c034          ]
[c1|c014           |c015                     |c16     |c016 |c017          ]
[c1|c014           |c015                     |c16     |c016 |c017          ]
[c1|c014           |c015                     |c16     |c016 |c017          ]
