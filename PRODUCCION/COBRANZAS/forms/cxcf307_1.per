c022 = FORMONLY.tit_col4, WIDGET = 'BUTTON', CONFIG = 'F18';
c023 = FORMONLY.tit_col5, WIDGET = 'BUTTON', CONFIG = 'F19';
c024 = FORMONLY.tit_col6, WIDGET = 'BUTTON', CONFIG = 'F20';

c030 = FORMONLY.tit_arean;
x2   = FORMONLY.cladoc;
c031 = FORMONLY.numdoc;
c032 = FORMONLY.nomcli;
c033 = FORMONLY.fecha TYPE DATE, FORMAT = "dd-mm-yyyy";
c034 = FORMONLY.valor      TYPE DECIMAL(12,2), FORMAT =  '####,###,##&.##';
c035 = FORMONLY.tit_venc, REVERSE;
c040 = FORMONLY.tot_doc    TYPE DECIMAL(14,2), FORMAT =  '####,###,##&.##';

c054 = FORMONLY.num_row, NOENTRY, UPSHIFT, REVERSE;
c055 = FORMONLY.max_row, NOENTRY, UPSHIFT, REVERSE;
INSTRUCTIONS
DELIMITERS '||'
SCREEN RECORD rm_doc[12] (FORMONLY.tit_arean, FORMONLY.cladoc, FORMONLY.numdoc,
			 FORMONLY.nomcli, FORMONLY.fecha, FORMONLY.valor)

KEYS
"F5"="Ver Factura"
"F6"="E/C Cliente"
"F7"="Documento"
