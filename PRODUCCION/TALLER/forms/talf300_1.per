c025 = FORMONLY.tit_col6, WIDGET = 'BUTTON', CONFIG = 'F20';
f    = FORMONLY.tit_col7, WIDGET = 'BUTTON', CONFIG = 'F21';

c010 = FORMONLY.tit_fecha, NOENTRY, UPSHIFT, FORMAT = 'dd-mm-yyyy',
	OPTIONS = '-nolist';
c    = FORMONLY.tit_tipo, NOENTRY, UPSHIFT, OPTIONS = '-nolist';
c012 = talt023.t23_orden, NOENTRY, UPSHIFT, OPTIONS = '-nolist';
c013 = talt023.t23_nom_cliente, NOENTRY, UPSHIFT, OPTIONS = '-nolist';
c014 = FORMONLY.tit_modelo_det, NOENTRY, UPSHIFT, OPTIONS = '-nolist';
c015 = talt023.t23_tot_neto, NOENTRY, UPSHIFT,
	FORMAT = '#,###,###,##&.##', OPTIONS = '-nolist';
e    = FORMONLY.estado, NOENTRY, UPSHIFT, OPTIONS = '-nolist';

c030 = talt023.t23_descripcion, UPSHIFT, NOENTRY, REVERSE;
c031 = FORMONLY.tit_total, NOENTRY, REVERSE, UPSHIFT,
	FORMAT = '#,###,###,##&.##';

c060 = FORMONLY.num_row, NOENTRY, UPSHIFT, REVERSE;
c061 = FORMONLY.max_row, NOENTRY, UPSHIFT, REVERSE;
