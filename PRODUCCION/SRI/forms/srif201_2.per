DATABASE FORMONLY
SCREEN
{

              Período: [c000] - [c0] 

}
ATTRIBUTES
c000 = FORMONLY.anio_fin TYPE SMALLINT NOT NULL, REQUIRED, AUTONEXT, UPSHIFT,
	REVERSE, INCLUDE = (2000 TO 2050),
	COMMENTS = 'Digite el año final en formato: aaaa.';
c0   = FORMONLY.mes_fin TYPE SMALLINT NOT NULL, REQUIRED, AUTONEXT, UPSHIFT,
	REVERSE, INCLUDE = (1 TO 12), FORMAT = '&&',
	COMMENTS = 'Digite el mes final en formato: mm.';
END
