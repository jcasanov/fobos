c023 = FORMONLY.tit_col4, WIDGET = 'BUTTON', CONFIG = 'F18';
c024 = FORMONLY.tit_col5, WIDGET = 'BUTTON', CONFIG = 'F19';
c025 = FORMONLY.tit_col6, WIDGET = 'BUTTON', CONFIG = 'F20';
c    = FORMONLY.tit_col7, WIDGET = 'BUTTON', CONFIG = 'F21';

c030 = actt010.a10_codigo_bien, NOENTRY, UPSHIFT;
c031 = actt010.a10_descripcion, NOENTRY, UPSHIFT;
c032 = actt010.a10_fecha_comp, NOENTRY, UPSHIFT, FORMAT = 'dd-mm-yyyy';
c033 = actt010.a10_porc_deprec, NOENTRY, UPSHIFT, FORMAT = '#&.##';
c034 = actt010.a10_valor_mb, NOENTRY, UPSHIFT, FORMAT = '##,###,##&.##';
c035 = actt010.a10_val_dep_mb, NOENTRY, UPSHIFT, FORMAT = '##,###,##&.##';
d    = actt010.a10_estado, NOENTRY, UPSHIFT;

c040 = FORMONLY.tot_valor_mb, NOENTRY, UPSHIFT, REVERSE,
	FORMAT = '##,###,##&.##';
c041 = FORMONLY.tot_val_dep_mb, NOENTRY, UPSHIFT, REVERSE,
	FORMAT = '##,###,##&.##';
END
