c012 = FORMONLY.n_cobrador, NOENTRY, REVERSE, UPSHIFT;
c013 = cxct024.z24_referencia, AUTONEXT, UPSHIFT;
c014 = cxct024.z24_total_cap, AUTONEXT, REVERSE, FORMAT = '#,###,###,##&.##';

c015 = cxct024.z24_usuario, REVERSE, NOENTRY, UPSHIFT;
c016 = cxct024.z24_fecing, REVERSE, NOENTRY, UPSHIFT;
