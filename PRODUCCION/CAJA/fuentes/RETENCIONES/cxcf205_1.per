{

                                                        Estado: [c|c000      ]

  No. Solicitud: [c001       ]
        Cliente: [c002  ] [c003                                              ]
                          [c003                                              ]

   Area Negocio: [c0] [c004            ]  G. Línea: [c005 ] [c006            ]
         Moneda: [c1] [c007            ]   Paridad: [c008               ]

        Subtipo: [c009] [c010                          ]    
