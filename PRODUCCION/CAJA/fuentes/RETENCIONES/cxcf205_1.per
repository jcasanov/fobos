	WIDGET = 'FIELD_BMP', CONFIG = 'lista.bmp F2';
c003 = FORMONLY.n_cliente, NOENTRY, REVERSE, UPSHIFT, WORDWRAP COMPRESS;
c0   = cxct024.z24_areaneg, AUTONEXT, REVERSE, UPSHIFT,
	WIDGET = 'FIELD_BMP', CONFIG = 'lista.bmp F2';
c004 = FORMONLY.n_areaneg, NOENTRY, UPSHIFT, REVERSE;
c005 = cxct024.z24_linea, AUTONEXT, UPSHIFT, REVERSE,
	WIDGET = 'FIELD_BMP', CONFIG = 'lista.bmp F2';
c006 = FORMONLY.n_linea, NOENTRY, UPSHIFT, REVERSE;
c1   = cxct024.z24_moneda, REVERSE, UPSHIFT, AUTONEXT,
